<?xml version="1.0" encoding="utf-8"?>
<com.example.cassandrakane.goalz.utils.VerticalPager
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_main_vertical_pager"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:fitsSystemWindows="true"
    android:orientation="vertical"
    tools:context=".MainActivity"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <fragment
        android:id="@+id/main_top_fragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        class="com.example.cassandrakane.goalz.NotificationsFragment" />

    <fragment
        android:id="@+id/main_central_fragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        class="com.example.cassandrakane.goalz.CentralFragment" />

</com.example.cassandrakane.goalz.utils.VerticalPager>